{
  "name": "backpacking",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "firebase emulators:exec \"vite\" --import ./firebase_emulator_persistence --export-on-exit",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "set NODE_ENV=test&& firebase emulators:exec \"vitest\" --import ./firebase_test_emulator_persistence",
    "simpletest": "set NODE_ENV=test&& firebase emulators:exec \"vitest run\" --import ./firebase_test_emulator_persistence",
    "coverage": "set NODE_ENV=test&& firebase emulators:exec \"vitest run --coverage\""
  },
  "dependencies": {
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-query-devtools": "^4.26.1",
    "firebase": "^9.17.1",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.8.1",
    "sort-by": "^0.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vitest/ui": "^0.29.2",
    "autoprefixer": "^10.4.13",
    "global-jsdom": "^8.7.0",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.21",
    "react-error-overlay": "^6.0.9",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.28.4"
  }
}
